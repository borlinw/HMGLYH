  <sql id="Qiaoliang_Where">
  	 <where>
			  	<!-- 路线 -->
			  	<if test="roadcode != null and roadcode != '' ">
			  		and roadcode = #{roadcode}
			  	</if>
			  	<!-- 桥梁编码 -->
			  	<if test="code != null and code != '' ">
			  		and code = #{code}
			  	</if>
			  	<!-- 行政区划 编码 -->
			  	<if test="xzqh != null and xzqh != '' ">
			  		and xzqh = #{xzqh}
			  	</if>
			  	
			  	<!-- 桥梁名称 -->
			  	<if test="name != null and name != '' ">
			  		and name like '%' || #{name} || '%'
			  	</if>
			  	
			  	<if test="startzh != null and startzh != ''">
			  		and #{startzh}  <![CDATA[ <= ]]> POS 
			  	</if>
			  	
			  	<if test="endzh != null and endzh != '' " >
			  		and POS <![CDATA[ <= ]]> #{endzh}
			  	</if>
			  	
			  	<if test="qlkjfl != null and qlkjfl != '' ">
			  		and qlkjfl like '%' || #{qlkjfl} || '%'
			  	</if>
			  	
			  	<if test="qlxz != null and qlxz != '' ">
			  		and qlxz like '%' || #{qlxz} || '%'
			  	</if>
			  	
			  </where>
			  order by roadcode,roadpos
  </sql>
  
